<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_15_80"
        >
        <ImageView 
            android:id="@+id/user_name_image"
            android:layout_width="@dimen/height_14_80"
       		android:layout_height="@dimen/height_17_80"
       		android:src="@drawable/user_pic"
       		android:layout_marginTop="@dimen/height_3_80"
       		android:layout_marginLeft="@dimen/width_2_80"
       		android:layout_marginBottom="@dimen/height_2_80"
            />
        <LinearLayout 
            android:layout_toRightOf="@id/user_name_image"
            android:layout_width="wrap_content"
       		android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <TextView 
                android:id="@+id/fragment_center_user_name"
                android:layout_marginTop="@dimen/height_5_80"
                android:layout_marginLeft="@dimen/width_2_80"
                android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:textSize="@dimen/text_size_20"
       			android:gravity="center"
                android:text="董晓燕"
                android:textColor="@color/fragment_center_user_name_text"
                />
            <TextView 
                android:id="@+id/fragment_center_grade_class"
                android:layout_marginLeft="@dimen/width_2_80"
                android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:layout_marginTop="@dimen/height_1_80"
                android:text="一年级   一班"
               	android:gravity="center"
                android:textSize="@dimen/text_size_16"
                android:textColor="@color/fragment_center_grade_class_text"
                />
        </LinearLayout>
    </RelativeLayout>
   <View
        android:id="@+id/fragment_center_divide_line"
        style="@style/fragment_center_divide_line"
        />
   <LinearLayout 
        android:layout_width="match_parent"
       	android:layout_height="wrap_content"
       	android:orientation="horizontal"
       >
       <RelativeLayout 
           	android:layout_weight="1"
            android:layout_width="wrap_content"
       		android:layout_height="@dimen/height_7_80">
           <ImageView 
                android:id="@+id/fragment_center_book_iamge_readed"
                android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:gravity="center"
       			android:layout_marginTop="@dimen/height_1_80"
       			android:layout_marginLeft="@dimen/width_7_80"
       			android:layout_marginBottom="@dimen/height_0_80"
                android:src="@drawable/readed_book_pic"
               />
           <TextView  
                android:id="@+id/fragment_center_readed_text_content"
               	android:layout_toRightOf="@id/fragment_center_book_iamge_readed" 
                android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:layout_centerInParent="true"
       			android:layout_marginLeft="@dimen/width_3_80"
       			android:gravity="center"
       			android:text="已读"
       			android:textSize="@dimen/text_size_16"
       			android:textColor="@color/fragment_center_readed_text_content"/>
           <TextView 
               	android:id="@+id/fragment_center_readed_text_content_count" 
                android:layout_toRightOf="@id/fragment_center_readed_text_content" 
                android:layout_width="wrap_content"
       		    android:layout_height="wrap_content"
       		    android:layout_marginLeft="@dimen/height_3_80"
       		    android:layout_centerInParent="true"
       		    android:gravity="center"
       		    android:text="12"
       		    android:textSize="@dimen/text_size_16"
       		    android:textColor="@color/fragment_center_readed_text_content_count"/>
       </RelativeLayout>
   		<View
   		android:layout_weight="0.008"
        android:id="@+id/fragment_center_divide_line"
        style="@style/fragment_center_height_divide_line"
        />
       <RelativeLayout 
           	android:layout_weight="1"
            android:layout_width="wrap_content"
       		android:layout_height="@dimen/height_7_80">
           <ImageView 
                android:id="@+id/fragment_center_book_iamge_reading"
                android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:gravity="center"
       			android:layout_marginTop="@dimen/height_1_80"
       			android:layout_marginLeft="@dimen/width_9_80"
       			android:layout_marginBottom="@dimen/height_0_80"
                android:src="@drawable/reading_book_pic"
               />
           <TextView  
                android:id="@+id/fragment_center_reading_text_content"
               	android:layout_toRightOf="@id/fragment_center_book_iamge_reading" 
                android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:layout_centerInParent="true"
       			android:layout_marginLeft="@dimen/width_3_80"
       			android:gravity="center"
       			android:text="在读"
       			android:textSize="@dimen/text_size_16"
       			android:textColor="@color/fragment_center_readed_text_content"/>
           <TextView 
               	android:id="@+id/fragment_center_reading_text_content_count" 
                android:layout_toRightOf="@id/fragment_center_reading_text_content" 
                android:layout_width="wrap_content"
       		    android:layout_height="wrap_content"
       		    android:layout_marginLeft="@dimen/height_3_80"
       		    android:layout_centerInParent="true"
       		    android:gravity="center"
       		    android:text="7"
       		    android:textSize="@dimen/text_size_16"
       		    android:textColor="@color/fragment_center_readed_text_content_count"/>
       </RelativeLayout>
   </LinearLayout>
    <View
        style="@style/fragment_center_divide_line"
        />
    <TextView 
        android:layout_height="@dimen/height_4_80"
        android:layout_width="match_parent"
        android:background="@color/fragment_center_divide_view_background"
        />
    <View
        style="@style/fragment_center_divide_line"
        />
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_10_80"
        >
        <ImageView 
            android:id="@+id/user_change_picture"
            android:layout_width="wrap_content"
       		android:layout_height="wrap_content"
       		android:src="@drawable/change_user_pic"
       		android:layout_centerVertical="true"
       		android:layout_marginTop="@dimen/height_2_80"
       		android:layout_marginLeft="@dimen/width_7_80"
            />
            <TextView 
                android:id="@+id/fragment_center_change_picture_text"
                android:layout_toRightOf="@id/user_change_picture"
                android:layout_marginTop="@dimen/height_4_80"
                android:layout_marginLeft="@dimen/width_3_80"
                android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:textSize="@dimen/text_size_14"
       			android:layout_centerInParent="true"
                android:text="修改图像"
                android:textColor="@color/fragment_center_change_picture_text"
                />
            <TextView
                android:id="@+id/fragment_center_change_right_chick_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/left_show_chick_pic"
               	android:layout_marginRight="@dimen/width_7_80"
                android:textColor="@color/fragment_center_grade_class_text"
                />
    </RelativeLayout>
  	<View
        style="@style/fragment_center_change_right_chick_tex_down_divide"
        />
  	    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_10_80"
        >
        <ImageView 
            android:id="@+id/user_change_picture"
            android:layout_width="wrap_content"
       		android:layout_height="wrap_content"
       		android:src="@drawable/change_password_pic"
       	    android:layout_centerVertical="true"
       		android:layout_marginTop="@dimen/height_2_80"
       		android:layout_marginLeft="@dimen/width_7_80"
            />
            <TextView 
                android:id="@+id/fragment_center_change_picture_text"
                android:layout_toRightOf="@id/user_change_picture"
                android:layout_marginTop="@dimen/height_4_80"
                android:layout_marginLeft="@dimen/width_3_80"
                android:layout_width="wrap_content"
       			android:layout_height="wrap_content"
       			android:textSize="@dimen/text_size_14"
       			android:layout_centerInParent="true"
                android:text="修改密码"
                android:textColor="@color/fragment_center_change_picture_text"
                />
            <TextView
                android:id="@+id/fragment_center_change_right_chick_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/left_show_chick_pic"
               	android:layout_marginRight="@dimen/width_7_80"
                android:textColor="@color/fragment_center_grade_class_text"
                />
    </RelativeLayout>
    	<View
        style="@style/fragment_center_change_right_chick_tex_down_divide"
        />
    	<TextView 
    	      android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:text="this is bank!"
              android:gravity="center"
              android:background="@color/fragment_center_last_text_back"
    	    
    	    />
</LinearLayout>
